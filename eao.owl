    <!-- http://eao.onl/ontologies/eao.owl#abstract -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#abstract">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#aspect"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasConventionColor"/>
                <owl:allValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#white"/>
