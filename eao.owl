    <!-- http://eao.onl/ontologies/eao.owl#resource -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#resource">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000031"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#has_abstract"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#strategy_layer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
