    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#active_structure_aspect">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#aspect"/>
        <rdfs:subClassOf>
