    <!-- http://eao.onl/ontologies/eao.owl#value_stream -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#value_stream">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000031"/>
