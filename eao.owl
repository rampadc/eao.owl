    <!-- http://eao.onl/ontologies/eao.owl#active_structure_aspect -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#active_structure_aspect">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#aspect"/>
        <rdfs:comment xml:lang="en">Represents the structural elements (the business actors, application components, and devices that display actual behavior; i.e., the “subjects” of activity)</rdfs:comment>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#active_structure_element -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#active_structure_element">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#is_classfied_as"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#active_structure_aspect"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#element"/>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#application_layer -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#application_layer">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#layer"/>
        <dc:source>https://pubs.opengroup.org/architecture/archimate3-doc/chap03.html</dc:source>
        <rdfs:comment>Depicts application services that support the business, and the applications that realize them</rdfs:comment>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#architecture -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#architecture">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#has_view"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#architecture_view"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The fundamental concepts or properties of a system in its environment embodied in its elements, relationships, and in pricinples of its design and evolution.

[Source: ISO/IEC/IEEE 42010:2011]

The structure of components, their inter-relationships, and the principles and guidelines governing their designs and evolution over time.

[Source: TOGAF 9 addition]</rdfs:comment>
        <rdfs:label xml:lang="en">architecture</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#architecture_view -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#architecture_view">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <obo:IAO_0000118 xml:lang="en">view</obo:IAO_0000118>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap02.html#_Toc10045273</dc:source>
        <rdfs:comment xml:lang="en">A representation of a system from the perspective of a related set of concerns.</rdfs:comment>
        <rdfs:label xml:lang="en">architecture view</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#aspect -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#aspect">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <rdfs:comment xml:lang="en">Classification of elements based on layer-independent characteristics related to the concerns of different stakeholders.</rdfs:comment>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#assessment_element -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#assessment_element">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#evaluates"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#driver"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#is_addressed_by"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#goal_element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#is_classfied_as"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#active_structure_aspect"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#is_classfied_as"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#motivation_aspect"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000112 xml:lang="en">opportunity in something</obo:IAO_0000112>
        <obo:IAO_0000112 xml:lang="en">weakness in something</obo:IAO_0000112>
        <rdfs:comment xml:lang="en">Represents the result of an analysis of the state of affairs of the enterprise with respect to some driver.

May reveal strengths, weaknesses, opportunities, or threats for some area of interest. These need to be addressed by adjusting existing goals or setting new ones, which may trigger changes to the Enterprise Architecture</rdfs:comment>
        <rdfs:label xml:lang="en">assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#behavior_aspect -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#behavior_aspect">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#aspect"/>
        <rdfs:comment xml:lang="en">Represents the behavior (processes, functions, events, and services) performed by the actors; structural elements are assigned to behavioral elements, to show who or what displays the behavior</rdfs:comment>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#behavior_element -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#behavior_element">
        <owl:equivalentClass>
