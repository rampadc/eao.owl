    <!-- http://eao.onl/ontologies/eao.owl#abstract -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#abstract">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#aspect"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasConventionColor"/>
                <owl:allValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#white"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap03.html</dc:source>
        <rdfs:comment xml:lang="en">non-instantiable, used only in the metamodel</rdfs:comment>
        <rdfs:label xml:lang="en">abstract</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#active_structure_aspect -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#active_structure_aspect">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#aspect"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasConventionColor"/>
                <owl:allValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#dark_grey"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap03.html</dc:source>
        <rdfs:comment xml:lang="en">represents the structural elements (the business actors, application components, and devices that display actual behavior; i.e., the “subjects” of activity)</rdfs:comment>
        <rdfs:label xml:lang="en">active structure aspect</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#active_structure_element -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#active_structure_element">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#structure_element"/>
        <owl:disjointWith rdf:resource="http://eao.onl/ontologies/eao.owl#passive_structure_element"/>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap04.html</dc:source>
        <rdfs:comment xml:lang="en">Active structure elements are the subjects that can perform behavior</rdfs:comment>
        <rdfs:label xml:lang="en">active structure element</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#application_layer -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#application_layer">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#layer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasConventionColor"/>
                <owl:allValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#blue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap02.html</dc:source>
        <rdfs:comment xml:lang="en">depicts application services that support the business, and the applications that realize them</rdfs:comment>
        <rdfs:label xml:lang="en">application layer</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#architecture -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#architecture">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0001019"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#architecture_view"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0001019"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0001019"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#layer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The fundamental concepts or properties of a system in its environment embodied in its elements, relationships, and in pricinples of its design and evolution.

[Source: ISO/IEC/IEEE 42010:2011]

The structure of components, their inter-relationships, and the principles and guidelines governing their designs and evolution over time.

[Source: TOGAF 9 addition]</rdfs:comment>
        <rdfs:label xml:lang="en">architecture</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#architecture_view -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#architecture_view">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap02.html</dc:source>
        <rdfs:comment xml:lang="en">A representation of a system from the perspective of a related set of concerns.

[Source: Archimate 3.1]</rdfs:comment>
        <rdfs:label xml:lang="en">architecture view</rdfs:label>
        <rdfs:label xml:lang="en">view</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#architecture_viewpoint -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#architecture_viewpoint">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap02.html</dc:source>
        <CommonCoreOntologies:alternative_label xml:lang="en">viewpoint</CommonCoreOntologies:alternative_label>
        <rdfs:comment>A specification of the conventions for a particular kind of architecture view.

[Source: Archimate 3.1]</rdfs:comment>
        <rdfs:label xml:lang="en">architecture viewpoint</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#aspect -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#aspect">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasConventionColor"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#aspect_color"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap02.html</dc:source>
        <rdfs:comment xml:lang="en">Classification of elements based on layer-independent characteristics related to the concerns of different stakeholders. Used for positioning elements in the ArchiMate metamodel.</rdfs:comment>
        <rdfs:label xml:lang="en">aspect</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#aspect_color -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#aspect_color">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#color"/>
        <owl:disjointWith rdf:resource="http://eao.onl/ontologies/eao.owl#layer_color"/>
        <rdfs:comment xml:lang="en">shades of grey are used to distinguish elements belonging to the different aspects of the ArchiMate framework</rdfs:comment>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#behavior_aspect -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#behavior_aspect">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#aspect"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasConventionColor"/>
                <owl:allValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#medium_grey"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap03.html</dc:source>
        <rdfs:comment xml:lang="en">represents the behavior (processes, functions, events, and services) performed by the actors; structural elements are assigned to behavioral elements, to show who or what displays the behavior</rdfs:comment>
        <rdfs:label xml:lang="en">behavior aspect</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#behavior_element -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#behavior_element">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#element"/>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap04.html</dc:source>
        <rdfs:label xml:lang="en">behavior element</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#blue -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#blue">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#layer_color"/>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#business_layer -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#business_layer">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#layer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasConventionColor"/>
                <owl:allValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#yellow"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap02.html</dc:source>
        <rdfs:comment xml:lang="en">depicts business services offered to customers, which are realized in the organization by business processes performed by business actors</rdfs:comment>
        <rdfs:label xml:lang="en">business layer</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#collaboration -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#collaboration">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#internal_active_structure_element"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#aggregates"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://eao.onl/ontologies/eao.owl#internal_active_structure_element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap04.html</dc:source>
        <rdfs:comment xml:lang="en">Represents an aggregate of two or more internal active structure elements, working together to perform some collective behavior.</rdfs:comment>
        <rdfs:label xml:lang="en">collaboration</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#color -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#color">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:comment xml:lang="en">convention colors, used to distinguish between the layers and aspects of the Archimate Core Framework</rdfs:comment>
        <rdfs:label xml:lang="en">color</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#composite_element -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#composite_element">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#element"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#aggregates"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#composite_element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasSpecialization"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#grouping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#hasSpecialization"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#isComposedOf"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#composite_element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap02.html</dc:source>
        <rdfs:comment xml:lang="en">an element consisting of other elements from multiple aspects or layers of the language</rdfs:comment>
        <rdfs:label xml:lang="en">composite element</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#corner_shape -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#corner_shape">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#accesses"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#passive_structure_element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#aggregates"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#internal_behavior_element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#flowsTo"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#flowsTo"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#internal_behavior_element"/>
            </owl:Restriction>
