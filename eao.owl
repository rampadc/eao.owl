                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#is_classfied_as"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#behavior_aspect"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#element"/>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#business_actor -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#business_actor">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000004"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#fulfills"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#business_role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#has_abstract"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#business_layer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#is_assigned_to"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#business_role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#is_classfied_as"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#active_structure_aspect"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Represents a business entity that is capable of performing behavior.</rdfs:comment>
        <rdfs:label>business actor</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#business_collaboration -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#business_collaboration">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000027"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#aggregates"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://eao.onl/ontologies/eao.owl#active_structure_element"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#aggregates"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://eao.onl/ontologies/eao.owl#business_element"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#has_abstract"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#business_layer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://eao.onl/ontologies/eao.owl#is_classfied_as"/>
                <owl:someValuesFrom rdf:resource="http://eao.onl/ontologies/eao.owl#active_structure_aspect"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
