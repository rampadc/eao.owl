            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap03.html</dc:source>
        <rdfs:comment xml:lang="en">represents the structural elements (the business actors, application components, and devices that display actual behavior; i.e., the “subjects” of activity)</rdfs:comment>
        <rdfs:label xml:lang="en">active structure aspect</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#active_structure_element -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#active_structure_element">
        <rdfs:subClassOf rdf:resource="http://eao.onl/ontologies/eao.owl#structure_element"/>
        <owl:disjointWith rdf:resource="http://eao.onl/ontologies/eao.owl#passive_structure_element"/>
        <dc:source xml:lang="en">https://pubs.opengroup.org/architecture/archimate3-doc/chap04.html</dc:source>
        <rdfs:comment xml:lang="en">Active structure elements are the subjects that can perform behavior</rdfs:comment>
        <rdfs:label xml:lang="en">active structure element</rdfs:label>
    </owl:Class>
    


    <!-- http://eao.onl/ontologies/eao.owl#application_layer -->

    <owl:Class rdf:about="http://eao.onl/ontologies/eao.owl#application_layer">
